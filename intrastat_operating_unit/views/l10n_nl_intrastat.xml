<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="l10n_nl_intrastat_report_form" model="ir.ui.view">
            <field name="name">l10n_nl.report.intrastat.OU</field>
            <field name="model">l10n_nl.report.intrastat</field>
            <field name="inherit_id" ref="l10n_nl_intrastat.l10n_nl_intrastat_report_form" />
